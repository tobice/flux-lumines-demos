{
  "name": "flux-lumines-demos",
  "version": "1.0.0",
  "description": "Few nice Lumines demos showing the capabilites of Flux and React.js",
  "scripts": {
    "default": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --content-base ./default/ --config ./default/webpack.js --progress --colors --port 9090 --hot --inline",
    "recording": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --content-base ./recording/ --config ./recording/webpack.js --progress --colors --port 9090 --hot --inline",
    "save-state": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --content-base ./save-state/ --config ./save-state/webpack.js --progress --colors --port 9090 --hot --inline",
    "share-state": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --content-base ./share-state/ --config ./share-state/webpack.js --progress --colors --port 9090 --hot --inline",
    "build-lumines": "./node_modules/webpack/bin/webpack.js --content-base ./ --config ./webpack/build-lumines.js --progress --colors",
    "sockets-client": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --content-base ./sockets/ --config ./sockets/webpack.js --progress --colors --port 9090 --hot --inline",
    "sockets-server": "./node_modules/babel/bin/babel-node ./sockets/socketsServer.js"
  },
  "keywords": [
    "react",
    "flux",
    "lumines"
  ],
  "author": "Tobiáš Potoček",
  "email": "tobiaspotocek@gmail.com",
  "license": "MIT",
  "dependencies": {
    "debug": "^2.2.0",
    "event-emitter": "^0.3.3",
    "flux-lumines": "tobice/flux-lumines",
    "mock-browser": "^0.90.33",
    "react": "^0.13.1",
    "ws": "^0.7.2"
  },
  "devDependencies": {
    "autoprefixer-loader": "^2.0.0",
    "babel": "^4.0.1",
    "babel-core": "^4.0.1",
    "babel-loader": "^4.0.0",
    "css-loader": "^0.12.0",
    "less-loader": "^2.2.0",
    "react-hot-loader": "^1.2.5",
    "style-loader": "^0.12.1",
    "webpack": "^1.5.3",
    "webpack-dev-server": "^1.8.0"
  }
}
